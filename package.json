{
  "name": "snomore",

  "version": "0.1.0",
  "private": true,
  "description": "",

  "engines": {
    "node":"14.x"
  },
  "main": "server/server.js",


  "dependencies": {
    "axios": "^0.17.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.0.2",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "twilio": "^3.58.0",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && node server.js",
    "start:dev": "concurrently \"cd server && nodemon server.js --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.17.5"
  }
}
